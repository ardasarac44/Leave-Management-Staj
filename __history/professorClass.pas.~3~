unit professorClass;

interface

type
  Professor = class
    public
      m_firstName,
      m_lastName,
      m_faculty,
      m_department,
      m_email : string;
      constructor create(firstname, lastname, faculty, department, email: string);
  end;

implementation
constructor Professor.create(firstname: string; lastname: string; faculty: string; department: string; email: string);
begin
  m_firstName := firstname;
  m_lastName := lastname;
  m_faculty := faculty;
  m_department := department;
  m_email := email;

end;
end.
